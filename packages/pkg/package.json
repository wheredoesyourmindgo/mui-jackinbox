{
  "name": "mui-jackinbox",
  "description": "A Material-UI component that wraps animate.css.",
  "repository": {
    "type": "git",
    "url": "https://github.com/wheredoesyourmindgo/mui-jackinbox"
  },
  "keywords": [
    "material-ui",
    "react",
    "animate.css"
  ],
  "version": "0.0.4",
  "main": "dist/index.js",
  "license": "MIT",
  "source": "src/index.ts",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build",
    "type-check": "tsc -p .",
    "install-peers": "install-peers -f",
    "build": "microbundle --jsx React.createElement --tsconfig tsconfig.json --jsxFragment React.Fragment",
    "dev": "yarn build watch",
    "clean": "rimraf {dist,.rts2*}"
  },
  "author": "Abe Hendricks<moemanchu@icloud.com>",
  "devDependencies": {
    "@types/react": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "install-peers-cli": "^2.2.0",
    "microbundle": "^0.13.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.11.3",
    "next": "^10.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "dependencies": {
    "clsx": "^1.1.1"
  }
}
